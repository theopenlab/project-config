[access "refs/heads/*"]
abandon = group broadview-ui-core
label-Code-Review = -2..+2 group broadview-ui-core
label-Workflow = -1..+1 group broadview-ui-core

[access "refs/tags/*"]
pushSignedTag = group broadview-ui-release

[receive]
requireChangeId = true
requireContributorAgreement = true

[submit]
mergeContent = true
