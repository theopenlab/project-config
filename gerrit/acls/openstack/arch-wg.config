[access "refs/heads/*"]
abandon = group arch-wg-core
label-Code-Review = -2..+2 group arch-wg-core
label-Workflow = -1..+1 group arch-wg-core

[access "refs/tags/*"]
pushSignedTag = group arch-wg-release

[receive]
requireChangeId = true
requireContributorAgreement = true

[submit]
mergeContent = true
