[access "refs/for/refs/*"]
pushMerge = group oslo-release

[access "refs/for/refs/heads/feature/*"]
pushMerge = group oslo-messaging-core

[access "refs/heads/*"]
abandon = group oslo-messaging-core
label-Code-Review = -2..+2 group oslo-messaging-core
label-Workflow = -1..+1 group oslo-messaging-core

[access "refs/heads/stable/*"]
abandon = group stable-maint-core
label-Code-Review = -2..+2 group stable-maint-core
label-Workflow = -1..+1 group stable-maint-core

[receive]
requireChangeId = true
requireContributorAgreement = true

[submit]
mergeContent = true
