[access "refs/heads/*"]
abandon = group pynotedb-core
label-Code-Review = -2..+2 group pynotedb-core
label-Workflow = -1..+1 group pynotedb-core

[access "refs/tags/*"]
pushSignedTag = group pynotedb-release

[receive]
requireChangeId = true
requireContributorAgreement = true

[submit]
mergeContent = true
